{
  "manifest_version": 3,
  "name": "Github Optimizer",
  "version": "0.0.1",
  "description": "",
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content/index.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/icons/*.png"
      ],
      "matches": [],
      "extension_ids": []
    }
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://github.com/*"
  ]
}